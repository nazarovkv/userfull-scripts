    <disabled-settings>
      <setting-ref ref="RUNNER_2" />
    </disabled-settings>
    <parameters>
      <param name="env.checkout.dir" value="git-repos-all" />
      <param name="reverse.dep.*.checkout.dir" value="git-repos-all" />
      <param name="reverse.dep.*.checkout.dir.common" value="local-build" />
