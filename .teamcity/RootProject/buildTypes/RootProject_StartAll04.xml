<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="5e06a4eb-46a6-4025-8437-03d0e9d8acec" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/2018.1/project-config.xsd">
  <name>Start All - 04</name>
  <description />
  <settings>
    <options>
      <option name="allowExternalStatus" value="true" />
      <option name="allowPersonalBuildTriggering" value="false" />
      <option name="checkoutMode" value="ON_SERVER" />
      <option name="maximumNumberOfBuilds" value="1" />
      <option name="showDependenciesChanges" value="true" />
